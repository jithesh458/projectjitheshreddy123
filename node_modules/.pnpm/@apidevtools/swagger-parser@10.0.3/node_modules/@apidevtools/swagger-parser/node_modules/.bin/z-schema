#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/z-schema@5.0.5/node_modules/z-schema/bin/z-schema/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/z-schema@5.0.5/node_modules/z-schema/bin/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/z-schema@5.0.5/node_modules/z-schema/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/z-schema@5.0.5/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/@apidevtools/swagger-parser@10.0.3/node_modules/@apidevtools/swagger-parser/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/@apidevtools/swagger-parser@10.0.3/node_modules/@apidevtools/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/@apidevtools/swagger-parser@10.0.3/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/@apidevtools/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../z-schema@5.0.5/node_modules/z-schema/bin/z-schema" "$@"
else 
  exec node  "$basedir/../../../../../../../z-schema@5.0.5/node_modules/z-schema/bin/z-schema" "$@"
fi
